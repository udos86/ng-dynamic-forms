{
  "name": "ng-dynamic-forms",
  "version": "1.4.31",
  "description": "A rapid form development library for Angular",
  "scripts": {
    "aot-compile:sample": "ngc -p tsconfig.sample.aot.json",
    "bundle:sample:rollup": "rollup -c rollup.config.js",
    "bundle:sample:webpack": "webpack --config ./webpack.config.js",
    "build:packages": "gulp build",
    "compile:package:core": "ngc -p ./packages/core/tsconfig.json",
    "compile:package:ui-basic": "ngc -p ./packages/ui-basic/tsconfig.json",
    "compile:package:ui-bootstrap": "ngc -p ./packages/ui-bootstrap/tsconfig.json",
    "compile:package:ui-foundation": "ngc -p ./packages/ui-foundation/tsconfig.json",
    "compile:package:ui-ionic": "ngc -p ./packages/ui-ionic/tsconfig.json",
    "compile:package:ui-kendo": "ngc -p ./packages/ui-kendo/tsconfig.json",
    "compile:package:ui-material": "ngc -p ./packages/ui-material/tsconfig.json",
    "compile:package:ui-ng-bootstrap": "ngc -p ./packages/ui-ng-bootstrap/tsconfig.json",
    "compile:package:ui-primeng": "ngc -p ./packages/ui-primeng/tsconfig.json",
    "gulp": "gulp",
    "lint:packages": "tslint -c ./tslint.json './packages/**/*.ts'",
    "start": "lite-server -c bs-config.js",
    "tests:e2e": "protractor protractor.conf.js",
    "tests:unit": "karma start karma.conf.js --browsers PhantomJS --single-run",
    "tests:unit:debug": "karma start karma.conf.js --browsers Chrome",
    "transpile:sample:es2015": "tsc -p tsconfig.sample.json -m es2015",
    "transpile:sample:commonjs": "tsc -p tsconfig.sample.json -m commonjs",
    "watch:packages": "gulp watch:packages",
    "watch:sample": "tsc -p tsconfig.sample.json -m commonjs -watch",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udos86/ng2-dynamic-forms.git"
  },
  "author": {
    "name": "Udo Sch√∂fer",
